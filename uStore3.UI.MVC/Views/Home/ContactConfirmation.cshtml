@model uStore3.UI.MVC.Models.ContactViewModel
@{
    ViewBag.Title = "Contact Confirmation";
}

<div id="contact-page" class="container">
    <div class="bg">
        <div class="row">
            <div class="col-sm-12">
                <h2 class="title text-center">Contact Us</h2>
            </div>
        </div>
        <div class="row">

            <div class="col-sm-8">
                <h4>
                    Thank you for contacting us, @Model.Name! We will get back to you soon.
                </h4>
            </div>
        </div>
    </div>
</div>